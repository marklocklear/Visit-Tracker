<h1>New authorization_period</h1>

<% form_for(@authorization_period) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :from_date %><br />
    <%= f.datetime_select :from_date %>
  </p>
  <p>
    <%= f.label :to_date %><br />
    <%= f.datetime_select :to_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', authorization_periods_path %>
